						true,
						false
					],
					"defaultValue": "true",
					"path": "returnUnassignedEmployees"
				},
				{
					"name": "includeAccrualPolicyDetails",
					"type": "boolean",
					"options": [
						true,
						false
					],
					"defaultValue": "true",
					"path": "where.includeAccrualPolicyDetails"
				},
				{
					"name": "includeBaseWages",
					"type": "boolean",
					"options": [
						true,
						false
					],
					"defaultValue": "true",
					"path": "where.includeBaseWages"
				},
				{
					"name": "isEmployeeAllowed",
					"type": "boolean",
					"options": [
						true,
						false
					],
					"defaultValue": "true",
					"path": "where.isEmployeeAllowed"
				},
				{
					"name": "onlyActivePerson",
					"type": "boolean",
					"options": [
						false,
						true
					],
					"defaultValue": "false",
					"path": "where.onlyActivePerson"
				},
				{
					"name": "extensionType",
					"description": "ALL",
					"type": "select",
					"options": [
						"accrual",
						"device",
						"employee",
						"scheduling",
						"timekeeping"
					],
					"defaultValue": "",
					"path": "where.extensionType"
				},
				{
					"name": "snapshotDate",
					"type": "date",
					"defaultValue": "+0",
					"path": "where.snapshotDate"
				},
				{
					"name": "personnumber",
					"description": "personnumber",
					"type": "multi-text",
					"defaultValue": [
						""
					],
					"path": "where.employees.values",
					"validation": {
						"minEntered": 1,
						"maxEntered": 10,
						"pattern": "^[a-zA-Z0-9]+$",
						"errorMessage": "Employee IDs must be alphanumeric and unique."
					}
				}
			],
			"requestProfile": {
				"failOnNoAssignment": true,
				"returnUnassignedEmployees": true,
				"where": {
					"employees": {
						"key": "personnumber",
						"values": [
							"string"
						]
					},
					"extensionType": "accrual",
					"includeAccrualPolicyDetails": true,
					"includeBaseWages": "string",
					"isEmployeeAllowed": true,
					"onlyActivePerson": true,
					"snapshotDate": "string"
				}
			}
		},
